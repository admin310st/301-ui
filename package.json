{
  "name": "301-ui",
  "private": true,
  "scripts": {
    "build:icons": "node scripts/build-icons.mjs",
    "dev": "npm run build:icons && vite",
    "build": "npm run build:icons && vite build",
    "build:analyze": "npm run build && start build/bundle-stats.html",
    "preview": "vite preview",
    "deploy": "npm run build && wrangler deploy",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/",
    "purge:report": "purgecss --css 'static/css/**/*.css' --content 'static/**/*.html' --safelist '/^btn--/', '/^btn-chip--/', '/^icon-/', '/^is-/', '/^has-/', 'dark', 'light' --rejected --output build/purge-report"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^22.10.2",
    "eslint": "^9.39.2",
    "purgecss": "^7.0.2",
    "rollup-plugin-visualizer": "^6.0.5",
    "svgo": "^3.3.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.54.0",
    "vite": "^5.4.21",
    "vitest": "^4.0.18",
    "wrangler": "^3.80.3"
  },
  "dependencies": {
    "punycode.js": "^2.3.1"
  }
}
